alias gs='git status -s'
alias gp='git pull'
alias gsp='git stash; gp; git stash pop'
alias gsu='git submodule update --init --recursive'
alias ga='git add'
alias gaa='git add -A'
alias gr='git reset'
alias gv='git remote -v'
alias gc='git checkout'
alias gcl='git clone'
alias gb='git branch'
alias gbl='git branch -v'
alias gbn='git checkout -b'
alias gbs='git branch --show'
alias gl='git log'
alias gd='git diff'
alias gt='git log --graph --oneline --all'
alias gcm='git commit -m'
alias gclm='git commit -m "gotta increase that github score"'
alias gcam='git commit -a --allow-empty-message -m ""'
alias gcmvz='git commit -m "edited .zshrc"'
alias gsm='git ls-files -m'
alias gam='ga $(gsm)'
alias gph='git push origin $(gbs)'
# `vvg` to edit
