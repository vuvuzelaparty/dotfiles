alias gs='git status -s'
alias gp='git pull'
alias gsp='git stash; gp; git stash pop'
alias gsu='git submodule update --init --recursive'
alias ga='git add'
alias gaa='ga -A'
alias gr='git reset'
alias gv='git remote -v'
alias gc='git checkout'
alias gcl='git clean -fd'
alias gb='git branch'
alias gbl='git branch -v'
alias gbn='gc -b'
alias gbs='gb --show'
alias gl='git log'
alias gd='git diff'
alias gt='gl --graph --oneline --all'
alias gcm='git commit'
alias gcmm='gcm -m'
alias gclm='gcmm "gotta increase that github score"'
alias gcmvz='ga .zshrc; gcmm "edited .zshrc"'
alias gsm='git ls-files -m'
alias gam='ga $(gsm)'
alias gph='git push'
# `vvg` to edit
